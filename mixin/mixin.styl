f(a=1, b=1)
	display flex
	for n, i in flex-start center flex-end space-between space-around
		if (i+1) == a
			justify-content n
	for n, i in flex-start center flex-end
		if (i+1) == b
			align-items n

for n, i in flex-start center flex-end space-between space-around
	for m, j in flex-start center flex-end
		.f{i+1}{j+1}
			f(i+1,j+1)

text-ellipsis()
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;

.text-ellipsis
	text-ellipsis()


